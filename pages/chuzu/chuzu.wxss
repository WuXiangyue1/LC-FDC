/*下拉菜单  */

/* 下面是tobar */

#swiper-tab {
  width: 100%;
  position: relative;
}

.swiper-tab {
  width: 100%;
  height:50rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: white;
}

.swiper-tab-list {

  font-size: 28rpx;
  display: inline-block;
  width:203rpx;
  color: #666;
  position: relative;
}
.icon-d{font-size: 20rpx;}
.on {
  color: #00c482;
}

.on i {
  border: 5px solid #00c482;
  border-top-color: #00c482;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  text-align: center;
}

.swiper-item {
  background: #fff;
  width: 100%;
  height: 600rpx;
  display: none;
  position: absolute;
  top: 77rpx;
  z-index: 333333;
  left: 0;
  animation: displays 1s;
}

@keyframes displays {
  from {
    height: 0px;
  }

  to {
    height: 600rpx;
  }
}

.swiper-beijing {
  width: 100%;
  height: 100%;
  background: #000;
  position: absolute;
  opacity: 0.5;
  top:185rpx;
  left:0;
  display: none;
  z-index: 888;
  animation: 1s;
}


